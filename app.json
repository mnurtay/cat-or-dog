{
  "expo": {
    "name": "cat-or-dog",
    "slug": "cat-or-dog",
    "privacy": "public",
    "sdkVersion": "32.0.0",
    "platforms": [
      "ios",
      "android"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "description": "Mobile application for React Native. The application classifies animals using machine learning. More precisely a cat or a dog.",
    "githubUrl": "https://github.com/nurtaims/cat-or-dog.git"
  }
}